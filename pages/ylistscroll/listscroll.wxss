view {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
.viewport {
    overflow-y: scroll;
    position: relative;
}

.scroll-list {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
}

